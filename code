    <script>
        var z=0
      function func()
                {
     var x=document.getElementById("num")
      var ans=Number(x.value)
      var d=(Math.ceil(Math.random()*10))%10
      console.log(d)
     z++;
      if(d==ans)
      alert(" number of attempt :" +z)

}

        </script>
